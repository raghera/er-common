<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://localhost:8080/decoupling/schemas/common" elementFormDefault="unqualified">
	<xs:import namespace="http://localhost:8080/decoupling/schemas/common" schemaLocation="../common/pa_common.xsd"/>
	<!--
	An er-response encapsulates all response packets that are returned by
	 ER's REST interface.
	-->
	<xs:element name="er-response">
		<xs:complexType>
			<xs:sequence>
				<!-- 
				A generic payload will have wrap all xml fragments that are sent
				to the REST interface. The payload type defines this generic payload.
				-->
				<xs:element name="payload" type="common:payloadType"/>
			</xs:sequence>
			<xs:attribute name="id" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:int"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="sub-id" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:int"/>
				</xs:simpleType>
			</xs:attribute>
			<!-- 	
			The id and sub-id fields identify the type of response that is encapsulated
			in  the payload. The client application will be able to use these values to 
		   bind the payload to the appropriate java object model.
			-->
		</xs:complexType>
	</xs:element>
	<!--
		This schema reprsesents the static constant values supplied by ER.
	 -->
	<xs:element name="er-constants">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="er-constant-group" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="er-constant" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="name" type="xs:string" use="required"/>
									<xs:attribute name="value" type="xs:string" use="required"/>
									<xs:attribute name="sub-value" type="xs:string" use="optional"/>
									<xs:attribute name="description" type="xs:string" use="optional"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="type" type="xs:string" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--
		End ERConstants
	-->
	<!--
		The error frament will wrap ERExceptions as well as exceptions thrown
		 by the REST interface layer. Errors will be mapped to their id to give client
		side context -->
	<xs:element name="error">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="cause" type="xs:string" minOccurs="0"/>
				<xs:element name="description" type="xs:string"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:string" use="required"/>
			<xs:attribute name="system-id" type="xs:string" use="optional"/>
			<xs:attribute name="type" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--
		End Error Definition
	-->
	<!--
		Begin Express Packages Definition
