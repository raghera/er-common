	<!--
        This xml structure represents the payload returned by the REST interface when a
        usage-authorisation has failed. It represents the options available to the given user
         in relation to how they can contain a given package. This structure should contain
        a rich enough set of data to facilitate a purchase-request for a given package.
        -->
	<xs:element name="purchase-options">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="reason-code" type="common:reason-codeType"/>
				<xs:element name="sub-reason-code" type="common:reason-codeType"/>
				<xs:element name="super-credit-balances" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="resource-balance" type="common:resource-BalanceType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="packages">
					<xs:complexType>
						<xs:sequence>
							<!--
                             Package mapping. Multiple packages can be returned if a usage-authorisaton
                            is unsuccessful.
                            -->
							<xs:element name="package" type="common:packageType" minOccurs="0" maxOccurs="unbounded"/>
							<!--
                             END Package mapping.
                            -->
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="has-promotions" type="xs:boolean"/>
				<xs:element name="promotional-packages" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="package-id" type="xs:string" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- 
		End Purchase Options
	-->
