<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://localhost:8080/decoupling/schemas/common"
	targetNamespace="http://localhost:8080/decoupling/schemas/common">
	<!-- Payload Definition -->
	<xs:complexType name="payloadType">
		<xs:sequence>
		<!-- QC10093 changed from skip to lax -->
			<xs:any processContents="lax" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<!-- END Payload Definition -->
	<!-- PricePoint definition -->
	<xs:complexType name="price-pointType">
		<xs:sequence>
			<xs:element name="id" type="xs:string" />
			<xs:element name="discount-promo-text" type="xs:string" />
			<xs:element name="net-rate" type="xs:double" />
			<xs:element name="pricing-text-1" type="xs:string" />
			<xs:element name="pricing-text-2" type="xs:string" />
			<xs:element name="standard-rate" type="xs:double" />
			<xs:element name="standard-rate-as-string" type="xs:string" />
			<xs:element name="price-point-id-link" type="xs:string" />
			<xs:element name="purchase-link-text" type="xs:string" />
			<xs:element name="promo-code" type="xs:string" />
			<xs:element name="fixed-expiry-date" type="xs:dateTime"
				minOccurs="0" />
			<xs:element name="is-active" type="xs:boolean" />
			<xs:element name="is-preview" type="xs:boolean" />
			<xs:element name="is-pre-order" type="xs:boolean" />
			<xs:element name="is-trial" type="xs:boolean" />
			<xs:element name="is-discount" type="xs:boolean" />
			<xs:element name="is-recurring-package" type="xs:boolean" />
			<xs:element name="interactive-flag" type="xs:string" />
			<xs:element name="drm-object" type="drm-objectType" />
			<xs:element name="charging-resource" type="charging-resourceType" />
			<xs:element name="is-valid-super-credit-option" type="xs:boolean" />
			<xs:element name="duration" type="durationType" />
			<xs:element name="order" type="xs:int" />
			<xs:element name="min-sub-period" type="xs:int" />
			<xs:element name="balance-impacts" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="balance-impact" type="charging-resourceType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="balance-impact-rates" minOccurs="0" type="balanceImpactRates" />
			<xs:element name="rate" type="xs:double" />
			<xs:element name="rate-as-string" type="xs:string" />
			<xs:element name="super-credit-price-points" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="super-credit-price-point" type="super-credit-price-pointType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="roaming-net-amount" type="xs:double"
				minOccurs="0" />
			<xs:element name="roaming-gross-amount" type="xs:double"
				minOccurs="0" />
			<xs:element name="roaming-type" type="xs:string" />
			<!-- REMEDY 7063 -->
			<xs:element name="payment-type" type="xs:int" minOccurs="0" />
			<!-- CR1803 - start -->
			<xs:element name="custom-fields" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="custom-field" type="customFieldType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- CR1803 - end -->
			<!-- MQC 7458 - add start and expiry dates to the pricepoint, these are only returned
			for the findPackagesWithService request when pricepoint-start-end-dates-required flag is set true on the request -->
			<xs:element name="start-date" type="xs:dateTime" minOccurs="0" />
			<xs:element name="expiry-date" type="xs:dateTime" minOccurs="0" />
			<!-- MQC 7458 - add user groups to the pricepoint, these are only returned
			for the findPackagesWithService request when pricepoint-user-groups-required flag is set true on the request -->
			<xs:element name="user-groups" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="user-group" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- END price mapping -->
		</xs:sequence>
	</xs:complexType>
	<!-- END PricePoint definition -->
	<!-- SuperPricePoint definition -->
	<xs:complexType name="super-credit-price-pointType">
		<xs:sequence>
			<xs:element name="rate-identifier" type="xs:string" />
			<xs:element name="super-credit-text" type="xs:string" />
			<xs:element name="is-active" type="xs:boolean" />
			<xs:element name="id" type="xs:string" />
			<xs:element name="discount-promo-text" type="xs:string" />
			<xs:element name="net-rate" type="xs:double" />
			<xs:element name="pricing-text-1" type="xs:string" />
			<xs:element name="pricing-text-2" type="xs:string" />
			<xs:element name="standard-rate" type="xs:double" />
			<xs:element name="standard-rate-as-string" type="xs:string" />
			<xs:element name="price-point-id-link" type="xs:string" />
			<xs:element name="purchase-link-text" type="xs:string" />
			<xs:element name="promo-code" type="xs:string" />
			<xs:element name="fixed-expiry-date" type="xs:dateTime"
				minOccurs="0" />
			<xs:element name="is-preview" type="xs:boolean" />
			<xs:element name="is-pre-order" type="xs:boolean" />
			<xs:element name="is-trial" type="xs:boolean" />
			<xs:element name="is-discount" type="xs:boolean" />
			<xs:element name="is-recurring-package" type="xs:boolean" />
			<xs:element name="interactive-flag" type="xs:boolean" />
			<xs:element name="drm-object" type="drm-objectType" />
			<xs:element name="charging-resource" type="charging-resourceType" />
			<xs:element name="is-valid-super-credit-option" type="xs:boolean" />
			<xs:element name="rate-as-string" type="xs:string" />
			<xs:element name="roaming-net-amount" type="xs:double"
				minOccurs="0" />
			<xs:element name="roaming-gross-amount" type="xs:double"
				minOccurs="0" />
			<xs:element name="roaming-type" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<!-- END SuperPricePoint definition -->
	<!-- ChargingResource mapping -->
	<xs:complexType name="charging-resourceType">
		<xs:sequence>
			<xs:element name="name" type="xs:string" />
			<xs:element name="translated-resource-name" type="xs:string" />
			<xs:element name="code" type="xs:string" />
			<xs:element name="is-usage-token" type="xs:boolean" />
			<xs:element name="is-pay-token" type="xs:boolean" />
			<xs:element name="is-resource" type="xs:boolean" />
			<xs:element name="is-currency" type="xs:boolean" />
			<xs:element name="resource-name" type="xs:string" />
			<xs:element name="resource-symbol" type="xs:string" />
			<xs:element name="is-super-credit" type="xs:boolean" />
		</xs:sequence>
	</xs:complexType>
	<!-- END ChargingResource mapping -->
	<!-- ChargingResource mapping -->
	<xs:complexType name="resource-BalanceType">
		<xs:sequence>
			<xs:element name="subscription-id" type="xs:string" />
			<xs:element name="oldest-subscription-id" type="xs:string" />
			<xs:element name="package-id" type="xs:string" />
			<xs:element name="threshold" type="xs:int" />
			<xs:element name="charging-resource" type="charging-resourceType" />
			<xs:element name="balance" type="xs:double" />
		</xs:sequence>
	</xs:complexType>
	<!-- END ChargingResource mapping -->
	<!-- Price definition -->
	<xs:complexType name="priceType">
		<xs:sequence>
			<xs:element name="amount" type="xs:string" />
			<xs:element name="currency" type="xs:string" />
			<xs:element name="duration" type="xs:string" />
			<xs:element name="units" type="xs:string" />
		</xs:sequence>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="price" />
					<xs:enumeration value="pre-ordered" />
					<xs:enumeration value="recurring" />
					<xs:enumeration value="promotional" />
					<xs:enumeration value="offer" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<!-- End Price definition -->
	<!-- DRM-Object definition -->
	<xs:complexType name="drm-objectType">
		<xs:sequence>
			<xs:element name="key" type="xs:string" />
			<xs:element name="id" type="xs:string" />
			<xs:element name="description" type="xs:string" />
			<xs:element name="drm-type" type="drm-typeType" />
		</xs:sequence>
	</xs:complexType>
	<!-- End DRM-Object definition -->
	<!-- DRM-type definition -->
	<xs:complexType name="drm-typeType">
		<xs:sequence>
			<xs:element name="code" type="xs:int" />
			<xs:element name="name" type="xs:string" />
			<xs:element name="is-drm" type="xs:boolean" />
			<xs:element name="is-jdp" type="xs:boolean" />
		</xs:sequence>
	</xs:complexType>
	<!-- End DRM-type definition -->
	<!-- Text definition -->
<!-- 	<xs:complexType name="textType" mixed="true">
		<xs:attribute name="name" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="offer-price" />
					<xs:enumeration value="special-offer" />
					<xs:enumeration value="subscribe-to" />
					<xs:enumeration value="usual-price" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="paragraph" />
					<xs:enumeration value="heading" />
					<xs:enumeration value="hard-return-line-item" />
					<xs:enumeration value="............." />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="style">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="italic" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType> -->
	<!-- End Text definition -->
	<!-- Package Definition -->
	<xs:complexType name="packageType">
		<xs:sequence>
			<xs:element name="id" type="xs:string" />
			<xs:element name="simple-package-id" type="xs:string" />
			<xs:element name="name" type="xs:string" />
			<xs:element name="description" type="xs:string" />
			<xs:element name="pricing-text-1" type="xs:string" />
			<xs:element name="pricing-text-2" type="xs:string" />
			<xs:element name="is-reserve-only" type="xs:boolean" />
			<xs:element name="package-type" type="xs:string" />
			<xs:element name="drm-type" type="drm-typeType" />
			<xs:element name="express-purchase" type="xs:boolean" />
			<xs:element name="is-receipting-flag" type="xs:boolean" />
			<xs:element name="is-event-package" type="xs:boolean" />
			<xs:element name="is-calendar-package" type="xs:boolean" />
			<xs:element name="is-recurring-package" type="xs:boolean" />
			<xs:element name="rate" type="xs:double" />
			<xs:element name="rate-as-string" type="xs:string" />
			<xs:element name="price-point" type="price-pointType" />
			<xs:element name="sub-price-point" type="price-pointType"
				minOccurs="0" />
			<xs:element name="is-price-point-ordered" type="xs:boolean" />
			<xs:element name="partner-logo" type="xs:string"
				minOccurs="0" />
			<xs:element name="partner-text" type="xs:string"
				minOccurs="0" />
			<xs:element name="price-points" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="price-point" type="price-pointType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- MQC 7458 add notification category -->
			<xs:element name="notification-category" type="xs:string" minOccurs="0" />
			<!-- MQC 7733 - package status -->
			<xs:element name="status" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- END Package Definition -->
	<!-- RatingAttributes mapping -->
	<xs:complexType name="ratingAttributesType">
		<xs:sequence>
			<xs:element name="asset-id" type="xs:string" minOccurs="0" />
			<xs:element name="content-name" type="xs:string"
				minOccurs="0" />
			<xs:element name="device-id" type="xs:string" minOccurs="0" />
			<xs:element name="device-type" type="xs:int" minOccurs="0" />
			<xs:element name="payment-type" type="xs:int" minOccurs="0" />
			<xs:element name="bearer" type="xs:string" minOccurs="0" />
			<xs:element name="event-date-time" type="xs:dateTime"
				minOccurs="0" />
			<!-- please note the is-interactive flag corresponds to disableInteractivity() 
				if set to false. -->
			<xs:element name="express-purchase-flag" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="is-interactive" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="promo-code" type="xs:string" minOccurs="0" />
			<xs:element name="device-name" type="xs:string" minOccurs="0" />
			<xs:element name="access-channel" type="xs:string"
				minOccurs="0" />
			<!-- CR-1791 -->
			<xs:element name="duration-code" type="xs:int" minOccurs="0" />
			<xs:element name="duration" type="durationType" minOccurs="0" />
			<xs:element name="payment-information" type="xs:string"
				minOccurs="0" />
			<xs:element name="network-code-string" type="xs:string"
				minOccurs="0" />
			<xs:element name="pre-rate" type="xs:double" minOccurs="0" />
			<!-- Added for CR2108 -->
			<xs:element name="renewal-pre-rate" type="xs:double" minOccurs="0" />
			<!-- added for CR 2145. Default value is false -->
			<xs:element name="pre-rate-price-is-gross" type="xs:boolean" minOccurs="0"/>
			<xs:element name="purchase-channel" type="xs:string"
				minOccurs="0" />
			<!-- TSM R10 -->
			<xs:element name="tax-rate" type="xs:double" minOccurs="0" />
			<!-- CR1231 -->
			<xs:element name="has-tax-rate" type="xs:boolean"
				minOccurs="0" />
			<!-- CR704 Affiliate ID -->
			<xs:element name="affiliate-id" type="xs:string"
				minOccurs="0" />
			<xs:element name="external-field1" type="xs:string"
				minOccurs="0" />
			<xs:element name="external-field2" type="xs:string"
				minOccurs="0" />
			<xs:element name="external-trans-id" type="xs:string"
				minOccurs="0" />
			<!-- CR-0978 Location Services -->
			<xs:element name="tariff" type="xs:string" minOccurs="0" />
			<!-- CR-0869 start -->
			<xs:element name="partner-id" type="xs:string" minOccurs="0" />
			<xs:element name="aggregator-id" type="xs:string"
				minOccurs="0" />
			<!-- CR-0869 end -->
			<!-- CR-1209 start -->
			<xs:element name="recipient-msisdn" type="xs:string"
				minOccurs="0" />
			<xs:element name="product-code" type="xs:string"
				minOccurs="0" />
			<!-- CR-1209 end -->
			<!-- CR-1448 start -->
			<xs:element name="merchant-name" type="xs:string"
				minOccurs="0" />
			<xs:element name="invoice-text" type="xs:string"
				minOccurs="0" />
			<!-- CR-1448 end -->
			<!-- MQC-5915 -->
			<xs:element name="channel" type="xs:int" minOccurs="0" />
			<!-- CR-1791 Start -->
			<xs:element name="short-package-id" type="xs:string"
				minOccurs="0" />
			<xs:element name="tax-code" type="xs:string" minOccurs="0" />
			<xs:element name="charging-method" type="xs:int"
				minOccurs="0" />
			<xs:element name="user-group" type="xs:string" minOccurs="0" />
			<xs:element name="has-historic-price-point-flag" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="history-start-date" type="xs:dateTime"
				minOccurs="0" />
			<!-- CR-1791 End -->
			<!-- CR-2134 add vendor id -->
			<xs:element name="vendor-id" type="xs:string" minOccurs="0" />
			<!-- MQC 7733 add csr id -->
			<xs:element name="csr-id" type="xs:string" minOccurs="0"/>
			<!-- CR 2255 add content category -->
			<xs:element name="content-category" type="xs:string" minOccurs="0"/>
			<!-- CR 2255 add partner info -->
			<xs:element name="partner-url" type="xs:string" minOccurs="0"/>
			<xs:element name="partner-contact-info" type="xs:string" minOccurs="0"/>
			<xs:element name="partner-email" type="xs:string" minOccurs="0"/>
			<!-- JIRA ET68 add partner name -->
			<xs:element name="partner-name" type="xs:string" minOccurs="0"/>
			<!-- JIRA ET391 Add external fields to ER_subscriptions and pass to ERIF & PNH -->
			<xs:element name="external-identifier1" type="xs:string" minOccurs="0"/>
			<xs:element name="external-identifier2" type="xs:string" minOccurs="0"/>
			<xs:element name="external-identifier3" type="xs:string" minOccurs="0"/>
			
		</xs:sequence>
	</xs:complexType>
	<!-- MQC 6569 Start Some RatingAttributes are only used in the Decoupling 
		Request but are not included in the response. To ensure that clients are 
		not broken, this additional type is created to enable us to return the missing 
		attributes which are related to the Transactions TransactionResponseAttributes 
		mapping -->
	<xs:complexType name="TransactionResponseAttributesType">
		<xs:sequence>
			<xs:element name="asset-id" type="xs:string" minOccurs="0" />
			<xs:element name="content-name" type="xs:string"
				minOccurs="0" />
			<xs:element name="payment-information" type="xs:string"
				minOccurs="0" />
			<xs:element name="affiliate-id" type="xs:string"
				minOccurs="0" />
			<xs:element name="external-field1" type="xs:string"
				minOccurs="0" />
			<xs:element name="external-field2" type="xs:string"
				minOccurs="0" />
			<xs:element name="external-trans-id" type="xs:string"
				minOccurs="0" />
			<xs:element name="partner-id" type="xs:string" minOccurs="0" />
			<xs:element name="aggregator-id" type="xs:string"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<!-- MQC 6569 End End TransactionResponseAttributes mapping -->
	<!-- ReasonCode mapping -->
	<xs:complexType name="reason-codeType">
		<xs:sequence>
			<xs:element name="code" type="xs:int" />
			<xs:element name="sub-code" type="xs:int" minOccurs="0" />
			<xs:element name="name" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<!-- End ReasonCode mapping -->
	<!-- Mappings to help map some service id related components which are part 
		of the subscription mapping -->
	<xs:complexType name="service-non-refund-descriptionType"
		mixed="true">
		<xs:attribute name="service-id" type="xs:string" use="required" />
	</xs:complexType>
	<xs:complexType name="service-provisioning-tagType"
		mixed="true">
		<xs:attribute name="service-id" type="xs:string" use="required" />
	</xs:complexType>
	<xs:complexType name="service-statusType" mixed="true">
		<xs:attribute name="service-id" type="xs:string" use="required" />
	</xs:complexType>
	<!-- END Mappings to help map some service id related components which are 
		part of the subscription mapping. -->
	<!-- Subscription Mapping -->
	<xs:complexType name="subscriptionType">
		<xs:sequence>
			<xs:element name="package" type="packageType" />
			<xs:element name="id" type="xs:string" />
			<xs:element name="start-date" type="xs:dateTime" />
			<xs:element name="purchase-date" type="xs:dateTime" />
			<xs:element name="expiry-date" type="xs:dateTime" />
			<xs:element name="has-expired" type="xs:boolean" />
			<xs:element name="resource-balances">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="resource-balance" type="resource-BalanceType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="is-recurring" type="xs:boolean" />
			<xs:element name="status" type="xs:int" />
			<xs:element name="translated-status" type="xs:string" />
			<xs:element name="rating-attributes" type="ratingAttributesType" />
			<xs:element name="final-min-subscription-end-date" type="xs:dateTime" />
			<xs:element name="current-number-of-occurences" type="xs:int" />
			<xs:element name="min-subscription-period-elapsed" type="xs:boolean" />
			<xs:element name="is-preordered" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="transactions" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="transaction" type="transactionType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="super-credit-id" type="xs:string" />
			<xs:element name="translated-super-credit-name" type="xs:string" />
			<xs:element name="super-credit-balances" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="resource-balance" type="resource-BalanceType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="overdue-expiry-date" type="xs:dateTime"
				minOccurs="0" />
			<xs:element name="has-valid-micro-service" type="xs:boolean" />
			<!-- MQC 4132 -->
			<xs:element name="services" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="service-id" type="xs:string"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- MQC 4132 -->
			<!-- MQC 5799 rollback -->
			<!-- MQC 5985 add back in -->
			<xs:element name="disallow-cancellations" type="xs:boolean"
				minOccurs="0" />
			<!-- CR2255 Phase2 - add partner information -->
			<xs:element name="b2b-partner" type="b2bPartnerType"
				minOccurs="0" />
			<!--  MQC 9655 - add parent package id -->
			<xs:element name="parent-package-id" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="micro-service-type">
		<xs:sequence>
