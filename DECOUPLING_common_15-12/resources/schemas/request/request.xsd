                            <xsd:element name="delivery-status" type="xsd:int"/>
                            <xsd:element name="event-units" type="xsd:double" minOccurs="0"/>
                            <!-- CR-0869 start-->
                            <xsd:element name="partner-id" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="aggregator-id" type="xsd:string" minOccurs="0"/>
                            <!-- CR-0869 end-->
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        End Usage Complete
    -->
    <!--
        This payload is sent to fulfill a get base prices.
     -->
    <xsd:element name="get-base-prices-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="service-id" type="xsd:string" maxOccurs="unbounded"/>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        get base prices
    -->
    <!--
        This payload is sent to retreive a single package based on it's id.
     -->
    <xsd:element name="get-package-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="id" type="xsd:string"/>
                <!-- MQC 7458 - flag to indicate package notification category is returned in the response -->
                <xsd:element name="notification-category-required" type="xsd:boolean" minOccurs="0"/>
                <!-- MQC 7733 - flag to indicate package status is returned in the response -->
                <xsd:element name="status-required" type="xsd:boolean" minOccurs="0"/>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        End Get Package Request
    -->
    <!--
    payload is sent to validate a service based on given service id.
    -->
    <xsd:element name="validate-service-request">
