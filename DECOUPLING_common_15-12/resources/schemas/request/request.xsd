    <!-- 
    CR-0978 Location Services 
    Modify Tariff API
    action can only have the value NEW or CHANGE or TERMINATE
    e.g. use cases
    For NEW - purchase all pricepoints with tariff A
    
        action = NEW
        sourceTariff = *
        destinationTariff = A
    
    
    For CHANGE - inactivate all subscriptins to tariff A & purchase all pricepoints with tariff B
    
        action = CHANGE
        sourceTariff = A
        destinationTariff = B
    
    
    For TERMINATE inactivate all subscriptins to tariff B
    
        action = TERMINATE
        sourceTariff = B
        destinationTariff = *
    -->

    <xsd:element name="modify-tariff-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="action" type="xsd:string"/>
                <xsd:element name="sourceTariff" type="xsd:string" minOccurs="0"/>
                <xsd:element name="destinationTariff" type="xsd:string" minOccurs="0"/>
                <xsd:element name="client-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="reason" type="xsd:string" minOccurs="0"/>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="get-tariff-request">
