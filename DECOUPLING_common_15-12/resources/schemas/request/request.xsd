            <xsd:sequence>
                <xsd:element name="id" type="xsd:string"/>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
    End validate Service Request
    -->
