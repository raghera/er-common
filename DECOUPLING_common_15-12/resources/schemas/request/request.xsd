    <!--
        get supercredit balances 
    -->
    <!--
    Renew package. This call allows the renewal of a subscription
    -->
    <xsd:element name="renew-purchase-package-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="client-id" type="xsd:string"/>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="subscription-id" type="xsd:string"/>
                <!-- pre-rate, external-trans-id and partner-id are in ratingAttributesType -->
                <xsd:element name="purchase-attributes" type="common:ratingAttributesType" minOccurs="0"/>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
                <!-- CR2237 flag for adding error-id to renew -->
                <xsd:element name="error-id-required" type="xsd:boolean" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        End Renew package
    -->
    <!--
    Rate Package. 
    -->
    <xsd:element name="rate-package-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="client-id" type="xsd:string"/>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="package-id" type="xsd:string"/>
                <xsd:element name="purchase-attributes" type="common:ratingAttributesType" minOccurs="0"/>
                <!-- CR1231 -->
