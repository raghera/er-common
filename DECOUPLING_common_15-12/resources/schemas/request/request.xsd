    <!--
        CR2068 This payload is sent to fulfill a get all services with partners request.
        This will return all serviceids with a list of there associated partner information
     -->
    <xsd:element name="get-all-services-partners-request"/>
    <!--
        CR2068 End
    -->

    <!--
        CR2082 This payload is sent to fulfill a validate msisdn request.
        This will make a validation call to the opco ERIF and return the result.
    -->
    <xsd:element name="validate-msisdn-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="service-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="partner-id" type="xsd:string" minOccurs="0"/>
                <!-- CR2134 add vendor id -->
                <xsd:element name="vendor-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!-- ET-521  Start -->
    <xsd:element name="validate-account-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="charging-id" type="common:chargingId"/>
                <xsd:element name="service-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="partner-id" type="xsd:string" minOccurs="0"/>
                <!-- CR2134 add vendor id -->
                <xsd:element name="vendor-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!-- ET-521  End -->
    <!--
      CR2082 End
    -->

    <!-- CR2040 MPAY Goodwill credit request -->
    <xsd:element name="goodwill-credit-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/> <!-- customer's MSISDN -->
                <xsd:element name="package-id" type="xsd:string"/> <!-- fully qualified package ID (price point) -->
                <xsd:element name="pre-rate" type="xsd:double"/> <!-- pre-rated amount to be credited -->
                <xsd:element name="partner-id" type="xsd:string"/> <!-- aggregator's ID -->
                <xsd:element name="merchant-id" type="xsd:string"/> <!-- merchant's ID -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="invoice-text" type="xsd:string" minOccurs="0"/> <!-- invoice text -->
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!-- CR2040 MPAY Goodwill credit request End -->

    <!-- CR2040 MPAY Modify spend limits -->
    <xsd:element name="modify-spend-limits-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/> <!-- customer's MSISDN -->
                <xsd:element name="spend-limits"> <!-- NEW spend limits -->
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="per-tx-limit" type="xsd:double"
                                        minOccurs="0"/> <!-- per transaction spend limit -->
                            <xsd:element name="per-day-limit" type="xsd:double"
                                        minOccurs="0"/> <!--   per day spend limit -->
                            <xsd:element name="per-month-limit" type="xsd:double"
                                        minOccurs="0"/> <!-- per month spend limit -->
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="csr-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!-- CR2040 MPAY Modify spend limits End -->

    <!-- CR2198 Add Child Service Provider Id to Account -->
    <xsd:element name="modify-child-sp-id">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="child-sp-id" type="xsd:string"/>
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!-- CR2198 Add Service Provider Type to Account -->
    <xsd:element name="modify-sp-type">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="sp-type" type="xsd:string"/>
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!--  JIRA ET-43 QC 9847 - moved from partner_request.xsdd  -->
    <xsd:element name="get-details-for-external-transaction-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="external-trans-id" type="xsd:string"/>
                <xsd:element name="partner-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!--  JIRA ET-43 QC 9847 - moved from partner_request.xsdd  -->
    <xsd:element name="get-details-for-external-subscription-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <xsd:element name="partner-id" type="xsd:string"/>
                <xsd:element name="external-sub-id" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!--
        JIRA ET86 get user groups list from the core DB
     -->
    <xsd:element name="get-user-groups">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="client-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!--
        JIRA ET148 Add SMS blacklist flag to opt out of courtesy SMS notifications
    -->
    <xsd:element name="modify-account">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="suppress-courtesy-notifications" type="xsd:boolean"/>
                <xsd:element name="csr-id" type="xsd:string" minOccurs="0"/>
                <!--in future to extend modify account with more fields, add stuff here. When you add any more fields then make sure to make the suppress-courtesy-notifications as minoccurs = 0.
                Also each new field added will need its own transaction type and individual entry in the ER_MODIFY table i.e. a future modify-account call could result in multiple entries in the ER_MODIFY table -->
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <!-- JIRA ET197 DE CTB migration -->
    <xsd:element name="modify-barring-status-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="client-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="bar-name" type="xsd:string"/>
                <xsd:element name="value" type="xsd:boolean"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="get-barring-status">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="client-id" type="xsd:string"/>
                <xsd:element name="msisdn" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <!-- JIRA ET196 Get account subscription promo-codes info -->
    <xsd:element name="get-account-subscription-promo-codes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="client-id" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!-- JIRA ET196 Inactivate subscription promo-code  -->
    <xsd:element name="inactivate-subscription-promo-code">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="client-id" type="xsd:string"/>
                <xsd:element name="subscription-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="package-id" type="xsd:string" minOccurs="0"/> <!-- CURRENTLY NOT SUPPORTED -->
                <xsd:element name="reason" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!-- JIRA ET-238 - new service offers call -->
    <xsd:element name="get-service-offers">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <!-- Convergence -->
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="service-ids" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!-- JIRA-ET451 - add get sibling accounts api -->
    <xsd:element name="get-sibling-accounts">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="charging-id" type="common:chargingId"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="get-full-accounts">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="ban" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!-- MQC 13714 Re-triggering provisioning requests api -->
    <xsd:element name="reprovision-subscription">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="charging-id" type="common:chargingId"/>
                <xsd:element name="subscription-id" type="xsd:string"/>
                <xsd:element name="provision-type" type="common:provisionType"/>
                <xsd:element name="context" type="xsd:string" minOccurs="0"/>
                <xsd:element name="service-ids" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

</xsd:schema>
