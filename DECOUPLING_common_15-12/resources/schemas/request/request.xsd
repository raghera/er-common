<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:common="http://localhost:8080/decoupling/schemas/common"
            elementFormDefault="unqualified">
    <xsd:import namespace="http://localhost:8080/decoupling/schemas/common" schemaLocation="../common/pa_common.xsd"/>
    <!--
    An er-request encapsulates all request packets that are sent to
     ER's REST interface.
    -->
    <xsd:element name="er-request">
        <xsd:complexType>
            <xsd:sequence>
                <!-- 
                A generic payload will have wrap all xml fragments that are sent
                to the REST interface. The payload type defines this generic payload.
                -->
                <xsd:element name="payload" type="common:payloadType" minOccurs="0"/>
                <!-- CR1231 -->
                <!-- <xsd:element name="log-id" type="xsd:string" minOccurs="0"/> -->
            </xsd:sequence>
            <xsd:attribute name="id" use="required">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int"/>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="client-application-id" use="required">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="purchase_locale" use="required">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="language_locale" use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="client-domain" use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="domain-translator-context" use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string"/>
                </xsd:simpleType>
            </xsd:attribute>
            <!-- by setting it to integer not int, we get an Object not a primitive type.
                This means jaxb will not marshall the value if it's not been set 
                (which we need for backwards compatibility)		-->
            <xsd:attribute name="version" use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:integer"/>
                </xsd:simpleType>
            </xsd:attribute>
            <!--
                id, client-application-id and purchase-locale are mandatory for all calls to the
                REST interface and are therefore defined as attributes on the
                er-request schema. Alll requests made to the REST interface will
                 have these attributes available.
            -->
            <!-- 	
                The two attributes below may be used if we decide to embed actions
                in the response xml. An action is literally a url that facilitaes the
                purchase workflow as seen in the link element of the WPI implementation.
                    
                -	client-domain represents the domain from which the REST interface is called.

                -	domain-translator-context defines the context of the web component which is
                    responsible for the REST communication on the Client side of the fence. 
            -->
        </xsd:complexType>
    </xsd:element>
    <!--
        Begin Express Package Request Definition
    -->
    <xsd:element name="express-package-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="service-ids">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="service-id" type="xsd:string" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="full-response-required" type="xsd:boolean" minOccurs="0"/>
                <xsd:element name="express-display-attributes">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="is-headline" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="is-option" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="true-options" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="service-id" type="xsd:string" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        End Express Package Request Definition
    -->

    <!-- CR1789 FindPackagesByServiceIdOneStep START -->
    <xsd:element name="onestep-package-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="service-ids">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="service-id" type="xsd:string" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!-- CR1789 FindPackagesByServiceIdOneStep END -->


    <!--
        The Inactivate Subscription maps to the inactivateSubscription
        method available on the CustcareApi. It's response can be found
        in the inactivate-subscription-response schema.
    -->
    <xsd:element name="inactivate-subscription">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <!-- Convergence -->
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="subscription-id" type="xsd:string"/>
                <xsd:element name="csr-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="reason" type="xsd:string" minOccurs="0"/>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>

                <!-- MQC6976 -->
                <xsd:element name="apply-penalty-charge" type="xsd:boolean" minOccurs="0"/>
                <!-- MQC 7622-->
                <xsd:element name="override-disallow-flag" type="xsd:boolean" minOccurs="0"/>
                <!-- JIRA-ET486 - Add optional context field to inactivate-subscription decoupling call -->
                <xsd:element name="context" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        End Inactivate Subscription
    -->
    <!--
        Modify Subscription Charging Method maps to the 
        modifySubscriptionChargingMethod method available 
        on the SelfcareApi. It's response can be found
        in the modify-subscription-charging-method-response schema.
    -->
    <xsd:element name="modify-subscription-charging-method">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="subscription-id" type="xsd:string"/>
                <xsd:element name="charging-method" type="xsd:int"/>
                <xsd:element name="device-type" type="xsd:int" minOccurs="0"/>
                <xsd:element name="csr-id" type="xsd:string" minOccurs="0"/>
                <xsd:element name="reason" type="xsd:string" minOccurs="0"/>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
                <!-- MQC6976 -->
                <xsd:element name="apply-penalty-charge" type="xsd:boolean" minOccurs="0"/>
                <!-- MQC9420 flag to immediately inactive a subscription if it is in grace or suspended status-->
                <xsd:element name="inactivate-grace-or-suspended-sub" type="xsd:boolean" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        End Modify Subscription Charging Method
    -->
    <!--
        A puchase represents the request payload that is sent to the REST interface
         to facilitate a purchase. Once again all purchase attributes have been mapped
        in the xml although the Gap Analysis suggests that not all of these are required
        by the purchase application. Is is prudent to map them all?
    -->
    <xsd:element name="purchase">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
                <!-- Convergence -->
                <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
                <xsd:element name="package-id" type="xsd:string" minOccurs="0"/>
                <!-- MD 19/4/12 - purchase attributes are common to some other calls so are moved to pa_common.xsdd -->
                <!-- external-sub-id is in purchaseAttributesType -->
                <xsd:element name="purchase-attributes" type="common:purchaseAttributesType" minOccurs="0"/>
                <!-- pre-rate, external-trans-id and partner-id are in ratingAttributesType -->
                <xsd:element name="rating-attributes" type="common:ratingAttributesType" minOccurs="0"/>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
                <!-- JIRA-ET567 - Add optional context field to purchase decoupling call -->
                <xsd:element name="context" type="xsd:string" minOccurs="0"/>

            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        End Purchase
    -->
    <!--
        Begin Selfcare Subscriptions Definition
    -->
    <xsd:element name="selfcare-subscriptions-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="device" type="xsd:int" minOccurs="0"/>
                <xsd:element name="subscription-filter" type="common:subscription-filterType" minOccurs="0"/>
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
                <!-- MQC 5985 -->
                <xsd:element name="enable-disallow-cancellations" type="xsd:boolean" minOccurs="0"/>
                <!-- CR2255 return b2b partner info if required -->
                <xsd:element name="b2b-partner-info-required" type="xsd:boolean" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>

    </xsd:element>
    <!--
        End Selfcare Subscriptions Definition
    -->
    <!--
        Begin Selfcare Micro Services Request Definition
    -->
    <xsd:element name="selfcare-micro-services-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="device" type="xsd:int" minOccurs="0"/>
                <xsd:element name="micro-service-filter">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="subscription-id" type="xsd:long"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!--
        End Selfcare Micro Services Request Definition
    -->
    <!-- 
        Begin Selfcare Transactions Definition
    -->
    <xsd:element name="selfcare-transactions-request">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="msisdn" type="xsd:string"/>
                <xsd:element name="device" type="xsd:int" minOccurs="0"/>
                <!--	
                    MQC 6569 - Added to return extra attributes if they are required.
                    Choose whether to  have some additional transaction attributes returned or not.
                -->
                <xsd:element name="transaction-attributes-required" type="xsd:boolean" minOccurs="0"/>

                <xsd:element name="transactions-filter" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="active-events-only" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="is-no-zero-cost-events-only" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="is-monetry-events-only" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="is-modify-events-only" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="is-refund-events-only" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="is-refund-payments-events-only" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="is-all-events" type="xsd:boolean" minOccurs="0"/>
                            <xsd:element name="transaction-id" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="subscription-id" type="xsd:string" minOccurs="0"/>
                            <!-- REMEDY 6509 - added start and end date -->
                            <xsd:element name="start-date" type="xsd:dateTime" minOccurs="0"/>
                            <xsd:element name="end-date" type="xsd:dateTime" minOccurs="0"/>
                            <!-- MQC 4858 -->
                            <xsd:element name="max-events" type="xsd:int" minOccurs="0"/>
                            <!-- //CR 1029 ACR - add partner id to transaction filter -->
                            <xsd:element name="partner-id" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <!-- CR1231 -->
                <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
                <!-- 
                    MQC 6834 - If the property API_PRICEPOINT_RETURN_RATE_AS_STRING_NET is set to true
                    the purchase rate returned is a net price, set this field to true to return an 
                    additional field purchase rate gross in the response. 
                -->
                <xsd:element name="purchase-rate-gross-required" type="xsd:boolean" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!-- 
        End Selfcare Transactions Definition
    -->
    <!--
           the service usage instance is only used for volume rating
    -->

    <xsd:complexType name="usageAuthRateRequestType">
        <xsd:annotation>
            <xsd:documentation>
                This is the request type for both usage-auth-rate and usage-auth-rate-plus requests
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="msisdn" type="xsd:string" minOccurs="0"/>
            <xsd:element name="charging-id" type="common:chargingId" minOccurs="0"/>
            <xsd:element name="service-id" type="xsd:string"/>
            <xsd:element name="usage-attributes" type="common:usageAttributesType" minOccurs="0"/>
            <xsd:element name="rating-attributes" type="common:ratingAttributesType" minOccurs="0"/>
            <!-- CR1231 -->
            <xsd:element name="log-id" type="xsd:string" minOccurs="0"/>
            <!-- CR1803 Unique Pricepoint Identifier -->
            <xsd:element name="pricepoint_custom_fields_required" type="xsd:boolean" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:element name="usage-auth-rate" type="usageAuthRateRequestType"/>

    <!-- like a usage-auth-rate but will have a more verbose response
     and it doesn't take an external subId -->
    <xsd:element name="usage-auth-rate-plus" type="usageAuthRateRequestType"/>


    <!-- based on usageAuthRequestType but with one more element at the end -->
    <xsd:complexType name="usageAuthRateChargeRequestType">
        <xsd:annotation>
            <xsd:documentation>
                This is the request type for both usage-auth-rate-charge and usage-auth-rate-charge-plus requests
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="usageAuthRateRequestType">
                <xsd:sequence>
                    <xsd:element name="external-sub-id" type="xsd:string" minOccurs="0"/>
                    <!-- JIRA-ET573 - Add optional context field to force purchase decoupling call -->
                    <xsd:element name="context" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="usage-auth-rate-charge" type="usageAuthRateChargeRequestType"/>

    <!--
        just like a UARC except the response is more verbose, and it doesn't take an external subId
        TODO extract the complex type and have one inherit the other
            the service usage instance is only used for volume rating
            
    -->
    <xsd:element name="usage-auth-rate-charge-plus" type="usageAuthRateChargeRequestType"/>


    <!--
